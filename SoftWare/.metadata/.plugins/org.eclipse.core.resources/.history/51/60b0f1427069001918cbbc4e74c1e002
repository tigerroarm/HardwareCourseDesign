#include "font_manage.h"

//setting键的图标
#define ICON_SETTING_X_SIZE 16
#define ICON_SETTING_Y_SIZE 16
#define ICON_SETTING_BYTE_X (ICON_SETTING_X_SIZE/8)
#define ICON_SETTING_BYTE_Y 16
#define ICON_SETTING_BYTES (ICON_SETTING_BYTE_X * ICON_SETTING_BYTE_Y)
alt_u8 icon_setting[ICON_SETTING_BYTES];

//home键的图标
#define ICON_HOME_X_SIZE 16
#define ICON_HOME_Y_SIZE 16
#define ICON_HOME_BYTE_X (ICON_HOME_X_SIZE/8)
#define ICON_HOME_BYTE_Y 16
#define ICON_HOME_BYTES (ICON_HOME_BYTE_X * ICON_HOME_BYTE_Y)
alt_u8 icon_home[ICON_HOME_BYTES];

//back键的图标
#define ICON_BACK_X_SIZE 16
#define ICON_BACK_Y_SIZE 16
#define ICON_BACK_BYTE_X (ICON_BACK_X_SIZE/8)
#define ICON_BACK_BYTE_Y 16
#define ICON_BACK_BYTES (ICON_BACK_BYTE_X * ICON_BACK_BYTE_Y)
alt_u8 icon_back[ICON_BACK_BYTES];
