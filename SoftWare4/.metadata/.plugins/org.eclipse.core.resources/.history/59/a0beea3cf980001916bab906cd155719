#include <stdio.h>
#include "system.h"
#include "altera_avalon_pio_regs.h"
#include "alt_types.h"

int main( void )
{
	alt_u16 lcd_data = 0x0000;
	printf( "lcd_data = %4x\n", lcd_data );

	IOWR_ALTERA_AVALON_PIO_DIRECTION(TFTLCD_DATA_BASE , 0xffff); //data设置为输出

	IOWR_ALTERA_AVALON_PIO_DATA(TFTLCD_DATA_BASE, lcd_data); // 写数据


	return 0;
}
